{
  "name": "quick-d",
  "version": "1.0.0",
  "description": "Quickly develop a nodejs background program",
  "main": "src/app.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon --exec babel-node ./src/app.js",
    "pro": "cross-env NODE_ENV=production  node ./dist/app.js",
    "build:external-lib": "babel ./lib -d ./dist/project-external-lib",
    "build": "babel ./src -d ./dist && npm run build:external-lib"
  },
  "author": "yijie",
  "license": "Apache License V2",
  "dependencies": {
    "colors": "^1.4.0",
    "figlet": "^1.5.0",
    "koa": "^2.13.0",
    "koa-bodyparser": "^4.3.0",
    "koa-router": "^9.4.0",
    "module-alias": "^2.2.2",
    "reflect-metadata": "^0.1.13"
  },
  "devDependencies": {
    "@babel/cli": "^7.11.6",
    "@babel/core": "^7.11.6",
    "@babel/node": "^7.10.5",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-proposal-decorators": "^7.10.5",
    "@babel/plugin-transform-runtime": "^7.12.1",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-flow": "^7.10.4",
    "@babel/runtime": "^7.12.1",
    "babel-plugin-parameter-decorator": "^1.0.16",
    "cross-env": "^7.0.2",
    "nodemon": "^2.0.5"
  }
}
